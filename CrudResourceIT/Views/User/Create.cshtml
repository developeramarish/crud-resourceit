
@{
	ViewBag.Title = "Create";
}


<h2>Criar Usuário</h2>

<div class="row">
	<div class="col-lg-6">
		<div class="form-group">
			<label for="basic-url">Nome</label>
			<div class="form-group">
				<input type="text" class="form-control" id="firstname" required aria-describedby="basic-addon3">
			</div>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-lg-6">
		<label for="basic-url">Sobrenome</label>
		<div class="form-group">
			<input type="text" class="form-control" id="lastname" required aria-describedby="basic-addon3">
		</div>
	</div>
</div>
<div class="row">
	<div class="col-lg-6">
		<label for="basic-url">Email</label>
		<div class="form-group">
			<input type="text" class="form-control" id="email" required aria-describedby="basic-addon3">
		</div>
	</div>
</div>

<div class="row">
	<div class="col-lg-6">
		<label for="basic-url">Endereço</label>
		<div class="form-group">
			<input type="text" class="form-control" id="address" required aria-describedby="basic-addon3">
		</div>
	</div>
</div>
<div class="row">
	<div class="col-lg-6">
		<label for="basic-url">Telefone</label>
		<div class="form-group">
			<input type="text" class="form-control" id="phone" required aria-describedby="basic-addon3">
		</div>
	</div>
</div>
<div class="col-md-12">
	<button type="button" onclick="create()" id="btn-cadastrar" class="btn btn-primary btn-lg">Cadastrar</button>
</div>

<script src="~/Scripts/app/create.js"></script>